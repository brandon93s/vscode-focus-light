{
	"tokenColors": [
		{
			"settings": {
				"background": "#f5f5f5",
				"foreground": "#5a5a59"
			}
		},
		{
			"name": "Hide",
			"scope": [
				"comment",
				"constant",
				"entity",
				"invalid",
				"keyword",
				"markup",
				"meta",
				"storage",
				"string",
				"support",
				"variable",
				"source"
			],
			"settings": {
				"foreground": "#dfdfdf"
			}
		},
		{
			"name": "Show",
			"scope": [
				"string.quoted",
				"meta.paragraph",
				"entity.name.section"
			],
			"settings": {
				"foreground": "#5a5a59"
			}
		},
		{
			"name": "Show LaTeX",
			"scope": [
				"text.tex.latex support.function.general.tex",
				"support.function.section.latex",
				"string.other.math.block.environment.latex",
				"string.other.math.block.environment.latex support.function.general.tex",
				"text.tex.latex string.other.math.tex",
				"text.tex.latex constant.other.general.math.tex",
				"text.tex.latex string.other.math.tex meta.group.braces.tex",
				"text.tex.latex invalid.illegal.string"
			],
			"settings": {
				"foreground": "#999b99"
			}
		},
		{
			"name": "Hide quoted strings in HTML/XML",
			"scope": [
				"string.quoted.double.html",
				"string.quoted.double.xml"
			],
			"settings": {
				"foreground": "#dfdfdf"
			}
		},
		{
			"name": "Markdown show",
			"scope": [
				"markup.raw.inline.markdown",
				"string.other.link.title.markdown",
				"string.other.link.description.markdown",
				"markup.quote.markdown"
			],
			"settings": {
				"foreground": "#5a5a59"
			}
		},
		{
			"name": "Markdown show subtle",
			"scope": [
				"markup.underline.link.markdown",
				"markup.underline.link.image.markdown"
			],
			"settings": {
				"foreground": "#999b99"
			}
		},
		{
			"name": "Markdown hide",
			"scope": [
				"meta.image.inline.markdown",
				"meta.link.inline.markdown"
			],
			"settings": {
				"foreground": "#dfdfdf"
			}
		}
	],
	"colors": {
		"editor.background": "#f5f5f5",
		"editorCursor.foreground": "#9a9b99",
		"editor.foreground": "#5a5a59",
		"editorWhitespace.foreground": "#e1e1e1",
		"editor.lineHighlightBackground": "#ececec",
		"editor.selectionBackground": "#dfdfdf"
	},
	"name": "focus light"
}